{
  "coc.preferences.bracketEnterImprove": false,
  "coc.preferences.formatOnSaveFiletypes": ["rust", "svelte", "typescript", "javascript", "css", "html"],
  "coc.preferences.useQuickfixForLocations": true,
  "codeLens.enable": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.enableSign": false,
  "list.extendedSearchMode": false,
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.inlayHints.typeHints": false,
  "suggest.fixInsertedWord": false,
  "suggest.maxCompleteItemCount": 32,
  "svelte.enable-ts-plugin": true
}
